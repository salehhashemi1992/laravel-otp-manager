{
    "name": "salehhashemi/laravel-otp-manager",
    "description": "Laravel OTP manager\n\n",
    "license": "MIT",
    "authors": [
        {
            "name": "Saleh Hashemi",
            "email": "salpars2004@gmail.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/salehhashemi1992/laravel-otp-manager",
    "keywords": ["Laravel", "OTP", "Authentication", "Two-Factor", "SMS", "Security"],
    "type": "library",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0|^11.0",
        "salehhashemi/laravel-configurable-cache": "^1.1"
    },
    "require-dev": {
        "laravel/pint": "^1.8",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Salehhashemi\\OtpManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Salehhashemi\\OtpManager\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint",
        "analyse": "vendor/bin/phpstan analyse"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Salehhashemi\\OtpManager\\OtpManagerServiceProvider"
            ]
        }
    }
}
